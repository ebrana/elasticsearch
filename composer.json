{
    "name": "ebrana/elasticsearch",
    "license": "proprietary",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Elasticsearch\\": "src/Elasticsearch"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "src/Elasticsearch/Tests/"
        }
    },
    "require": {
        "php": ">=8.3.0",
        "elasticsearch/elasticsearch": "~v8.12.0",
        "doctrine/collections": "~2.1.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "~1.10.47",
        "phpunit/phpunit": "10.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
